{
    "cli": {
        "version": ">= 16.9.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal",
            "android": {
                "gradleCommand": ":app:assembleRelease",
                "env": {
                    "ABI_FILTERS": "arm64-v8a,armeabi-v7a"
                }
            }
        },
        "production": {
            "autoIncrement": true
        }
    },
    "submit": {
        "production": {}
    }
}
